<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
<!-- Servlet de validar -->
  <servlet>
    <servlet-name>validarDni</servlet-name>
    <servlet-class>es.iesptocruz.victor.servlet.ServletValidarDni</servlet-class>
  </servlet>

<servlet-mapping>
  <servlet-name>validarDni</servlet-name>
  <url-pattern>/validarDni</url-pattern>
</servlet-mapping>

<!-- Servlet de actualizar -->
<servlet>
    <servlet-name>actualizarDni</servlet-name>
    <servlet-class>es.iesptocruz.victor.servlet.ServletActualizar</servlet-class>
  </servlet>

<servlet-mapping>
  <servlet-name>actualizarDni</servlet-name>
  <url-pattern>/actualizarDni</url-pattern>
</servlet-mapping>

</web-app>
